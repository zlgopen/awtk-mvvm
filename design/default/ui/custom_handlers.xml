<window children_layout="default(r=7, c=1, m=10, s=20)" v-model="custom_handlers">
  <button text="Toast" v-on:click="{show_toast}"/>
  <button text="Info" v-on:click="{show_info}"/>
  <button text="Confirm" v-on:click="{show_confirm}"/>
  <button text="Choose File For Save" v-on:click="{choose, Args=file_for_save}"/>
  <button text="Choose File For Open" v-on:click="{choose, Args=file_for_open}"/>
  <button text="Choose Folder" v-on:click="{choose, Args=folder}"/>
  <button text="Open MyWindow" v-on:click="{open_mywindow}"/>
</window>
