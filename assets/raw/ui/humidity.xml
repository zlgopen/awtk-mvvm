<window anim_hint="htranslate" v-model="humidity.js">
  <label text="1" x="center" y="30" w="90%" h="40" text="humidity"/>
  <label text="1" x="center" y="middle" w="50%" h="40" v-data:text="{humidity}"/>
  <slider value="1" x="center" y="middle:40" w="80%" h="20" v-data:value="{humidity}"/>
  <button text="Close" x="center" y="bottom:40" w="40%" h="40" v-on:click="{nothing, CloseWindow=True}"/>
</window>
